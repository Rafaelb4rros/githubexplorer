{
  "compilerOptions": {
    "target": "es2016",                                 
    "module": "commonjs",                               
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "jsx": "react-jsx",                         
    "skipLibCheck": true                                
  },
  "include": [
      "./src/**/*"
  ],
  "exclude": [
      "node_modules"
  ]
}
